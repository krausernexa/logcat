<?xml version="1.0" encoding="utf-8"?>
<VoiceCommands 
  xmlns="http://schemas.microsoft.com/voicecommands/1.0" 
  xmlns:x="http://schemas.thinkman.com/voicecommands/extensions/1.0">

  <Domain Name="Calendar"  ClassName="CreateAppointmentRes" CortanaOnly="true">

    <Intent Name="CreateAppointment" Scope="g:Nop" EmitGrxml="false">

      <ListenFor> create [an] appointment </ListenFor>

    </Intent>


    <!-- Ask For Appointment Title Dialog -->
    <Intent Name="CreateAppointment" Scope="Title">
      <!-- Listen Fors -->
      <ListenFor> {TitleDictation} </ListenFor>

    </Intent>

    <Dictation Label="TitleDictation" SlotName="Title" />


    <!-- Ask For Start Date Time Dialog -->
    <Intent Name="CreateAppointment" Scope="StartTime">
      
      <!-- starts and ends -->
      <ListenFor> [[it] starts] {StartTimeStandalone} </ListenFor>
      <ListenFor> [[it] starts] {StartTimeStandalone} and [it] {goes for|lasts} {Duration} </ListenFor>
      <ListenFor> [[it] starts] {StartTimeStandalone} and [it] ends {EndTimeStandalone} </ListenFor>

      <ListenFor> [[it] starts] [on] {StartDate} </ListenFor>
      <ListenFor> [[it] starts] [on] {StartDate} and [it] {goes for|lasts} {Duration} </ListenFor>
      <ListenFor> [[it] starts] [on] {StartDate} and [it] {goes|lasts} {AllDay} </ListenFor>
      <ListenFor> [[it] starts] [on] {StartDate} and [it] ends [on] {EndDate} </ListenFor>

      <ListenFor> [[it] starts] {StartTime} [on] {StartDate} </ListenFor>
      <ListenFor> [[it] starts] [on] {StartDate} {StartTime} </ListenFor>

      <ListenFor> [[it] starts] {StartTime} [on] {StartDate} and [it] ends {EndTimeStandalone} </ListenFor>
      <ListenFor> [[it] starts] [on] {StartDate} {StartTime} and [it] ends {EndTimeStandalone} </ListenFor>

      <ListenFor> [[it] starts] {StartTime} [on] {StartDate} and [it] ends {EndTime} [on] {EndDate} </ListenFor>
      <ListenFor> [[it] starts] [on] {StartDate} {StartTime} and [it] ends {EndTime} [on] {EndDate} </ListenFor>

      <ListenFor> [[it] starts] {StartTime} [on] {StartDate} and [it] ends [on] {EndDate} {EndTime} </ListenFor>
      <ListenFor> [[it] starts] [on] {StartDate} {StartTime} and [it] ends [on] {EndDate} {EndTime} </ListenFor>

      <ListenFor> [[it] starts] {StartTime} [on] {StartDate} and [it] {goes for|lasts} {Duration} </ListenFor>
      <ListenFor> [[it] starts] [on] {StartDate} {StartTime} and [it] {goes for|lasts} {Duration} </ListenFor>

      <!-- from/to -->
      <ListenFor> [[it] goes] [from] {StartTimeFromTo} {to|till} {EndTimeFromTo} </ListenFor>
      <ListenFor> [[it] goes] [from] {StartDate} {to|till} {EndDate} </ListenFor>

      <ListenFor> [[it] goes] [from] {StartTimeFromTo} {to|till} {EndTimeFromTo} [on] {StartDate} </ListenFor>
      <ListenFor> [[it] starts] [on] {StartDate} [from] {StartTimeFromTo} {to|till} {EndTimeFromTo} </ListenFor>

      <ListenFor> [[it] goes] [from] {StartTime} [on] {StartDate} {to|till} {EndTime} [on] {EndDate} </ListenFor>
      <ListenFor> [[it] goes] [from] {StartDate} {StartTime} {to|till} {EndTime} [on] {EndDate} </ListenFor>

      <ListenFor> [[it] goes] [from] {StartTime} [on] {StartDate} {to|till} {EndDate} {EndTime} </ListenFor>
      <ListenFor> [[it] goes] [from] {StartDate} {StartTime} {to|till} {EndDate} {EndTime} </ListenFor>

      <!-- All day -->
      <ListenFor> {AllDay} </ListenFor>
      <ListenFor> [on] {StartDate} {AllDay} </ListenFor>
      <ListenFor> {AllDay} [on] {StartDate} </ListenFor>

    </Intent>

    <PhraseList Label="StartDate" SlotName="Date">
      <Item out="rules.latest()"> {g:CalendarDate} </Item>
    </PhraseList>

    <PhraseList Label="StartTime" SlotName="StartTime">
      <Item out="rules.latest()"> [at] {g:TimeHoursMinutesLimited}      </Item>
      <Item out="rules.latest()"> [in the] {g:TimePeriodMornAftEve}       </Item>
      <Item out="rules.latest()"> the {g:TimePeriodMornAftEve} of       </Item>
      <Item out="rules.latest()"> [at] {g:TimePeriodNight} [time]       </Item>
    </PhraseList>

    <PhraseList Label="EndDate" SlotName="EndDate">
      <Item out="rules.latest()"> {g:CalendarDate} </Item>
    </PhraseList>

    <PhraseList Label="EndTime" SlotName="EndTime">
      <Item out="rules.latest()"> [at] {g:TimeHoursMinutesLimited}      </Item>
      <Item out="rules.latest()"> [in the] {g:TimePeriodMornAftEve}       </Item>
      <Item out="rules.latest()"> the {g:TimePeriodMornAftEve} of       </Item>
      <Item out="rules.latest()"> [at] {g:TimePeriodNight} [time]       </Item>
    </PhraseList>

    <!-- The [Start|End]TimeStandalone PhraseLists are used when a time is specified without a date. -->
    <PhraseList Label="StartTimeStandalone" SlotName="StartTime">
      <Item out="rules.latest()"> [at] {g:TimeHoursMinutesLimited}    </Item>
      <Item out="rules.latest()"> {g:TimeOfDayWithoutDaysOfWeek}      </Item>
    </PhraseList>

    <PhraseList Label="EndTimeStandalone" SlotName="EndTime">
      <Item out="rules.latest()"> [at] {g:TimeHoursMinutesLimited}    </Item>
      <Item out="rules.latest()"> {g:TimeOfDayWithoutDaysOfWeek}      </Item>
    </PhraseList>

    <!-- The [Start|End]TimeFromTo PhraseLists are used in "from {StartTime} to {EndTime}"
         scenarios, to allow for phrases like "from morning to night" -->
    <PhraseList Label="StartTimeFromTo" SlotName="StartTimeFromTo">
      <Item out="rules.latest()"> {g:TimeHoursMinutesLimited}     </Item>
      <Item out="rules.latest()"> {g:TimeOfDay}                   </Item>
      <Item out="rules.latest()"> {g:TimeOfDayMornAftEveNight}    </Item>
    </PhraseList>

    <PhraseList Label="EndTimeFromTo" SlotName="EndTimeFromTo">
      <Item out="rules.latest()"> {g:TimeHoursMinutesLimited}     </Item>
      <Item out="rules.latest()"> {g:TimeOfDay}                   </Item>
      <Item out="rules.latest()"> {g:TimeOfDayMornAftEveNight}    </Item>
    </PhraseList>

    <PhraseList Label="Duration" SlotName="Duration">
      <Item out="rules.latest()"> {g:RelativeTime} </Item>
    </PhraseList>

    <!-- Ask For Start Time Dialog -->
    <Intent Name="CreateAppointment" Scope="CreateAppointment_AskForStartTime">
      <!-- Listen Fors -->
      <ListenFor> [[it] starts] {StartTime} </ListenFor>
      <ListenFor> [[it] starts] {StartTime} and [it] {goes for|lasts} {Duration} </ListenFor>

      <ListenFor> {it's|it is|is} {AllDay} </ListenFor>
      <ListenFor> [it] goes {AllDay} </ListenFor>

    </Intent>

    <PhraseList Label="AllDay" SlotName="AllDay">
      <Item Value="true"> all day </Item>
    </PhraseList>


    <!-- Ask For End Time Dialog -->
    <Intent Name="CreateAppointment" Scope="CreateAppointment_AskForEndTime">
      <!-- Listen Fors -->
      <ListenFor> [[it] ends] {EndTime} </ListenFor>

    </Intent>


